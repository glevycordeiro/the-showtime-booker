<%= simple_form_for Booking.new do |f| %>
  <%= f.association :movie_session ,collection: @sorted_sessions, label_method: :start_time, as: :radio_buttons %>
<% end %>


<%= form_for Booking.new do |f| %>
  <%= f.label :start_time %>:
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.submit %>
<% end %>

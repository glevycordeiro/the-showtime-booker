<%= simple_form_for @movie_session do |f| %>
  <%= f.association :movie %>
  <%= f.input :start_time, minute_step: 10 %>
  <%= f.input :start_date, as: :string, input_html: { class: "datepicker" } %>
  <%= f.input :end_date, as: :string, input_html: { class: "datepicker" } %>
  <%= f.submit "Go", class: "btn btn-primary" %>
<% end %>
